{
    "name": "timothydc/laravel-lightspeed-ecom-api",
    "description": "Connect with the Lightspeed eCom API",
    "keywords": [
        "timothydc",
        "lightspeed",
        "ecom",
        "laravel"
    ],
    "homepage": "https://github.com/timothydc/laravel-lightspeed-ecom-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Timothy De Cort",
            "email": "mail@timothydc.be"
        }
    ],
    "require": {
        "php": "^7.4",
        "seoshop/seoshop-php": "^1.9"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "TimothyDC\\LightpeedEcomApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TimothyDC\\LightpeedEcomApi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TimothyDC\\LightpeedEcomApi\\Laravel Lightpeed Ecom ApiServiceProvider"
            ],
            "aliases": {
                "LightpeedEcomApi": "TimothyDC\\LightpeedEcomApi\\LightpeedEcomApi"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
